# MongoDB 配置（使用线上服务）
MONGODB_URI=mongodb://admin:password@your-mongodb-host:27017/notemark?authSource=admin

# MinIO 配置（使用线上服务）
MINIO_ENDPOINT=your-minio-host
MINIO_PORT=9000
MINIO_ACCESS_KEY=your_access_key
MINIO_SECRET_KEY=your_secret_key
MINIO_USE_SSL=false
MINIO_BUCKET_NAME=note-mark
MINIO_URL=http://your-minio-host:9000/note-mark/

# 应用配置
NODE_ENV=production
PORT=6090

# HTTP 代理配置（可选，用于访问外部 API）
# 如果需要代理访问 881903.com 等外部网站，请取消下面的注释
HTTP_PROXY=http://host.docker.internal:7890
HTTPS_PROXY=http://host.docker.internal:7890
